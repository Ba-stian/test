{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "no-tabs": "off",
    "indent": ["error", "tab"],
    "import/newline-after-import": ["error", { "count": 2 }]
  }
}
